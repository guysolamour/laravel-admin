APP_NAME={{ application }}
APP_FIRST_NAME={{ app_first_name | default('admin') }}
APP_LAST_NAME={{ app_last_name | default('admin') }}
APP_ENV=production
APP_KEY={{ app_key.stdout }}
APP_DEBUG=false
APP_URL=https://{{ domain }}

LOG_CHANNEL=stack


DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE={{ database_name }}
DB_USERNAME={{ database_user }}
DB_PASSWORD={{ vault_database_password }}


BROADCAST_DRIVER=log
CACHE_DRIVER=redis
QUEUE_CONNECTION=sync
SESSION_DRIVER=redis
SESSION_LIFETIME=120

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379


FTP_HOST="{{ ftp_host  | default('') }}"
FTP_USERNAME="{{ ftp_username  | default('') }}"
FTP_PASSWORD="{{ ftp_password  | default('') }}"

MODEL_CACHE_ENABLED="{{ model_cache_enabled  | default('true') }}"
COOKIE_CONSENT_ENABLED=true

MAIL_MAILER=smtp
MAIL_HOST=127.0.0.1
MAIL_PORT=25
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="{{ mail_from_address  | default('admin@administrable.com') }}"


MAIL_DKIM_SELECTOR=dkim
MAIL_DKIM_DOMAIN={{ domain }}
MAIL_DKIM_PASSPHRASE=''
MAIL_DKIM_ALGORITHM=rsa-sha256
MAIL_DKIM_IDENTITY=null

MAIL_FROM_NAME="${APP_NAME}"

PUSHER_APP_ID=
PUSHER_APP_KEY=
PUSHER_APP_SECRET=
PUSHER_APP_CLUSTER=mt1

MIX_PUSHER_APP_KEY="${PUSHER_APP_KEY}"
MIX_PUSHER_APP_CLUSTER="${PUSHER_APP_CLUSTER}"
