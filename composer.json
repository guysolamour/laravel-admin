{
  "name": "guysolamour/administrable",
  "description": "An administration package based on Laravel. This package was made according to my personal use to not repeat the same things for each project.",
  "license": "MIT",
  "keywords": [
    "admin","laravel","administration"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Guy-roland ASSALE",
      "email": "rolandassale@gmail.com",
      "homepage": "https://roland-assale.info",
      "role": "developer"
    }
  ],
  "require": {
    "php": ">=7.2",
    "bmatovu/multi-auth": "^8.0",
    "creativeorange/gravatar": "^1.0",
    "kris/laravel-form-builder": "^1.21",
    "davejamesmiller/laravel-breadcrumbs": "^5.3",
    "mercuryseries/flashy": "^1.0",
    "unisharp/laravel-filemanager": "^2",
    "cviebrock/eloquent-sluggable": "^6.0",
    "laravel/helpers": "^1.1",
    "illuminate/support": "^6.0"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Guysolamour\\Administrable\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Guysolamour\\Administrable\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Guysolamour\\Administrable\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
