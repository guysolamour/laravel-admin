{
  "name": "guysolamour/laravel-administrable",
  "description": "An administration package based on Laravel. This package was made according to my personal use to not repeat the same things for each project.",
  "license": "MIT",
  "keywords": [
    "admin","laravel","administration"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Guy-roland ASSALE",
      "email": "rolandassale@gmail.com",
      "homepage": "https://roland-assale.info",
      "role": "developer"
    }
  ],
  "require": {
    "php": "^8.1.0",
    "laravel/ui": "^4.0",
    "laravel/framework": "^10.0",
    "laravel/telescope": "^4.14",
    "artesaos/seotools": "^1.1.0",
    "league/flysystem-ftp": "^3.0",
    "spatie/laravel-backup": "^8.1",
    "creativeorange/gravatar": "^1.0",
    "spatie/laravel-settings": "^2.8",
    "spatie/laravel-honeypot": "^4.3",
    "kris/laravel-form-builder": "^1.51",
    "spatie/laravel-permission": "^5.10",
    "guysolamour/laravel-dkim": "^1.0",
    "spatie/laravel-medialibrary": "^10.8",
    "guysolamour/laravel-commands": "^4.0",
    "cviebrock/eloquent-sluggable": "^10.0",
    "spatie/laravel-cookie-consent": "^3.2",
    "genealabs/laravel-model-caching": "^0.13"
  },
  "require-dev": {
    "orchestra/testbench": "^7.0",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Guysolamour\\Administrable\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Guysolamour\\Administrable\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Guysolamour\\Administrable\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
