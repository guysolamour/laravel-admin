{
  "name": "guysolamour/laravel-administrable",
  "description": "An administration package based on Laravel. This package was made according to my personal use to not repeat the same things for each project.",
  "license": "MIT",
  "keywords": [
    "admin","laravel","administration"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Guy-roland ASSALE",
      "email": "rolandassale@gmail.com",
      "homepage": "https://roland-assale.info",
      "role": "developer"
    }
  ],
  "require": {
    "php": ">=7.1",
    "bmatovu/multi-auth": "^7.3",
    "creativeorange/gravatar": "^1.0",
    "cviebrock/eloquent-sluggable": "^4.8",
    "davejamesmiller/laravel-breadcrumbs": "^5.2",
    "illuminate/support": "~5.8.0",
    "kris/laravel-form-builder": "^1.20",
    "mercuryseries/flashy": "^1.0",
    "unisharp/laravel-filemanager": "^1.9"
  },
  "require-dev": {
    "orchestra/testbench": "~3.8.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Guysolamour\\Admin\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Guysolamour\\Admin\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Guysolamour\\Admin\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
